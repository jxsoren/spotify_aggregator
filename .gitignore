# Ignore bundler config.
/.bundle

# Ignore gems installed via Bundler.
/vendor/bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
/tmp/sockets/*
/tmp/cache/*
!/tmp/pids/
!/tmp/sockets/
!/tmp/cache/
!/tmp/pids/.keep
!/tmp/sockets/.keep
!/tmp/cache/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore non-versioned files
/public/assets
/public/packs
/public/packs-test

# Ignore node_modules
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore dotenv environment variables files.
/.env
/.env.*
/.env.local
/.env.development.local
/.env.test.local
/.env.production.local

# Ignore Spring preloader state.
/.spring/*
!/spring/.keep

# Ignore Capybara artifacts
/capybara-*.html

# Ignore byebug history.
/.byebug_history

# Ignore IDE-specific files (JetBrains, VSCode, etc.)
.idea/*
.vscode/*
*.sublime*

# Ignore macOS specific files
.DS_Store

# Ignore coverage reports generated by tools like SimpleCov
/coverage/
/coverage/*

# Ignore the spec directory when running Rails generators
spec/
